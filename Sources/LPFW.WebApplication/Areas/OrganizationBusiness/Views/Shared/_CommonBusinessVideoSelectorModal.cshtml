<!-- 普通的上传视频文件 Modal -->
<div class="modal fade" id="commonBusinessVideoeSelectorModal" tabindex="-1" role="dialog" aria-labelledby="commonBusinessVideoesSelectorModal" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color:whitesmoke">
                <h5 class="modal-title">上传视频文件</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="" style="color:red">
                <input type="file" class="form-control col-md-7 col-xs-12" id="commonBusinessVideoSelector" name="commonBusinessVideoSelector" placeholder="请选择视频文件" value="" required>
                <input type="hidden" name="relVideoObjectID" id="relVideoObjectID" />      <!--这是一个隐藏的数据标签，用于转移待图片关联对象的ID-->
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="appCloseCommonBusinessVideoSelectorModal()">关闭并刷新数据</button>
            </div>
        </div>
    </div>
</div>

